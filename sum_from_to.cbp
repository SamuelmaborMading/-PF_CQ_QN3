#include <iostream>
using namespace std;

// Function to calculate the sum of all integers between first and last
int sum_from_to(int first, int last) {
    int sum = 0;

    for (int i = first; i <= last; i++) {
        sum += i;  // Add each number between first and last to sum
    }

    return sum;  // Return the total sum
}

int main() {
    int first, last;

    // Ask the user for the first and last numbers
    cout << "Enter the first number: ";
    cin >> first;
    cout << "Enter the last number: ";
    cin >> last;

    // Call the sum_from_to function and print the result
    int result = sum_from_to(first, last);
    cout << "The sum of all integers between " << first << " and " << last << " is: " << result << endl;

    return 0;
}

